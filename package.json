{
  "name": "colliv",
  "displayName": "Colliv",
  "description": "Collaborate with others easily on the same file",
  "version": "0.0.2",
  "publisher": "Collive",
  "icon": "Assets/Collive.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/samuel12209/Colliv-Extension.git"
  },
  "engines": {
    "vscode": "^1.96.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:colliv.startServer",
    "onCommand:colliv.joinSession"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "colliv.startServer",
        "title": "Start Colliv Server"
      },
      {
        "command": "colliv.joinSession",
        "title": "Join Colliv Session"
      },
      {
        "command": "colliv.stopSession",
        "title": "Stop Hosting Colliv Session"
      },
      {
        "command": "colliv.leaveSession",
        "title": "Leave Colliv Session"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run package",
    "compile": "webpack",
    "watch": "tsc -watch -p ./",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "yarn run compile-tests && yarn run compile && yarn run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/localtunnel": "^2.0.4",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/vscode": "^1.96.0",
    "@types/ws": "^8.5.13",
    "@typescript-eslint/eslint-plugin": "^8.17.0",
    "@typescript-eslint/parser": "^8.17.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "eslint": "^9.16.0",
    "ts-loader": "^9.5.1",
    "typescript": "^5.7.3",
    "webpack": "^5.95.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "localtunnel": "^2.0.2",
    "net": "^1.0.2",
    "ngrok": "^5.0.0-beta.2",
    "node-fetch": "^3.3.2",
    "os": "^0.1.2",
    "ws": "^8.18.0"
  },
  "galleryBanner": {
    "color": "#007acc",
    "theme": "dark"
  },
  "screenshots": [
    "Assets/default_ip_demo.png",
    "Assets/default_port_demo.png",
    "Assets/demo_pic.png"
  ]
}
